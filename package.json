{
  "name": "glasify-litle",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && next build",
    "ultra": "npx ultracite check",
    "ultra:fix": "npx ultracite fix",
    "ultra:unsafe": "npx ultracite fix --unsafe",
    "ultra:doc": "npx ultracite doctor",
    "format": "npx biome format --write",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:auth": "./scripts/test-auth.sh",
    "test:auth:unit": "vitest tests/unit/auth",
    "test:auth:integration": "vitest tests/integration/auth",
    "test:auth:e2e": "playwright test e2e/auth",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "logger:test": "tsx scripts/test-logger.ts",
    "logs:clean": "rm -f logs/*.log",
    "logs:view": "tail -f logs/combined.log",
    "logs:errors": "tail -f logs/error.log"
  },
