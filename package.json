{
  "name": "glasify-litle",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && next build",
    "lint": "npx ultracite check",
    "lint:fix": "npx ultracite fix",
    "lint:unsafe": "npx ultracite fix --unsafe",
    "lint:errors": "npx biome check --diagnostic-level=error",
    "format": "npx biome format --write",
    "ultra:doc": "npx ultracite doctor",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "seed": "tsx prisma/seed-cli.ts",
    "seed:minimal": "tsx prisma/seed-cli.ts --preset=minimal",
    "seed:demo": "tsx prisma/seed-cli.ts --preset=demo-client",
    "seed:full": "tsx prisma/seed-cli.ts --preset=full-catalog",
    "seed:glass-types": "tsx prisma/seeders/glass-types.seeder.ts",
    "seed:glass-solutions": "tsx prisma/seeders/glass-solutions.seeder.ts",
    "seed:glass-taxonomy": "pnpm seed:glass-solutions && pnpm seed:glass-types",
    "migrate:glass-taxonomy": "tsx scripts/migrate-glass-taxonomy.ts",
    "migrate:glass-taxonomy:dry": "tsx scripts/migrate-glass-taxonomy.ts --dry-run",
    "migrate:glass-taxonomy:tenant": "tsx scripts/migrate-glass-taxonomy.ts --tenant-id=",
    "rollback:glass-taxonomy": "tsx scripts/rollback-glass-taxonomy.ts",
    "rollback:glass-taxonomy:dry": "tsx scripts/rollback-glass-taxonomy.ts --dry-run",
    "validate:seed-data": "tsx scripts/validate-seed-data.ts",
    "validate:seed-data:strict": "tsx scripts/validate-seed-data.ts --strict",
    "validate:seed-data:types": "tsx scripts/validate-seed-data.ts --file=glass-types",
    "validate:seed-data:solutions": "tsx scripts/validate-seed-data.ts --file=glass-solutions",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:auth": "./scripts/test-auth.sh",
    "test:auth:unit": "vitest tests/unit/auth",
    "test:auth:integration": "vitest tests/integration/auth",
    "test:auth:e2e": "playwright test e2e/auth",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "logger:test": "tsx scripts/test-logger.ts",
    "logs:clean": "rm -f logs/*.log",
    "logs:view": "tail -f logs/combined.log",
    "logs:errors": "tail -f logs/error.log",
    "migrate:glass-chars": "tsx scripts/migrate-glass-characteristics.ts",
    "migrate:glass-chars:dry": "tsx scripts/migrate-glass-characteristics.ts --dry-run",
    "migrate:price-history": "tsx scripts/create-initial-price-history.ts",
    "migrate:price-history:dry": "tsx scripts/create-initial-price-history.ts --dry-run"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@formkit/tempo": "^0.1.2",
    "@hookform/resolvers": "5.2.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "6.17.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-pdf/renderer": "^4.3.1",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-table": "8.21.3",
    "@trpc/client": "11.6.0",
    "@trpc/react-query": "11.6.0",
    "@trpc/server": "11.6.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "^1.1.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "0.546.0",
    "next": "15.5.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.0",
    "react-day-picker": "9.11.1",
    "react-dom": "19.2.0",
    "react-hook-form": "7.64.0",
    "react-phone-number-input": "^3.4.12",
    "react-scan": "^0.4.3",
    "recharts": "2.15.4",
    "server-only": "0.0.1",
    "sonner": "^2.0.7",
    "superjson": "2.2.2",
    "tailwind-merge": "3.3.1",
    "use-debounce": "10.0.6",
    "vaul": "^1.1.2",
    "winston": "3.18.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@hookform/devtools": "^4.4.0",
    "@playwright/test": "1.56.0",
    "@tailwindcss/postcss": "4.1.14",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.7.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "5.0.4",
    "dotenv": "^17.2.3",
    "jsdom": "27.0.0",
    "lefthook": "1.13.6",
    "lint-staged": "16.2.3",
    "playwright": "1.56.0",
    "postcss": "8.5.6",
    "prisma": "6.17.0",
    "shadcn": "3.4.2",
    "tailwindcss": "4.1.14",
    "tsx": "4.20.6",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "ultracite": "5.6.4",
    "vitest": "3.2.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.17.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm dlx ultracite fix"
    ]
  }
}
